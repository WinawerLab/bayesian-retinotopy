       RowBox[{"Prepend", "[", 
        RowBox[{"name", ",", "$FigureRoot"}], "]"}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"FileNameJoin", "[", 
       RowBox[{"{", 
        RowBox[{"$FigureRoot", ",", "name"}], "}"}], "]"}]}], "]"}]}]}], 
