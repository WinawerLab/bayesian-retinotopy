        RowBox[{"\"\<PNG\>\"", "|", "\"\<PDF\>\""}], ":", "\"\<PNG\>\""}]}], 
      "]"}], ",", "code_"}], "]"}], ":=", 
   RowBox[{"With", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"sym", "=", 
       RowBox[{"TemporarySymbol", "[", "\"\<fig\>\"", "]"}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
